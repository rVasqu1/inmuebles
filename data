SUBMIT rvfakt01
  WITH vkorg   = 'Y051'                     " Organización de ventas
  WITH fkdat-low = '20250101'              " Fecha desde
  WITH fkdat-high = '20250531'             " Fecha hasta
  WITH matnr-low = 'MSDD123'               " Material
  WITH matnr-high = 'MSD768'               " Hasta el último
  AND RETURN.
