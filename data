DATA: lt_list   TYPE TABLE OF abaplist,
      lv_text   TYPE string.

SUBMIT rfreisradocitem
  WITH bukrs = '1000'
  WITH s_budat IN ('20250101' TO '20250531')
  WITH s_matnr IN ('MSDD123', 'MSD432', 'MSD768')
  EXPORTING LIST TO MEMORY
  AND RETURN.

CALL FUNCTION 'LIST_FROM_MEMORY'
  TABLES
    listobject = lt_list
  EXCEPTIONS
    not_found  = 1
    OTHERS     = 2.

IF sy-subrc = 0.
  CALL FUNCTION 'LIST_TO_ASCI'
    EXPORTING
      list_index         = -1
    IMPORTING
      list_ascii         = lv_text
    TABLES
      listasci           = lt_list
    EXCEPTIONS
      others             = 1.

  WRITE: / 'Contenido del reporte:', lv_text.
ENDIF.
